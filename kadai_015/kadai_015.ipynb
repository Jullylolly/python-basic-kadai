      "source": [
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    # 名前と年齢を出力するメソッド\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンスを作成\n",
        "person = Human(\"hon\", 58)\n",
        "\n",
        "# メソッドを使って名前と年齢を出力\n",
        "person.printinfo()\n"
      ]
    }
  ],
